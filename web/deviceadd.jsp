<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@taglib prefix="mytag" uri="/WEB-INF/MyTag"%><!DOCTYPE html><html lang="zh-cn"><%@include file="WEB-INF/jspf/head.jsp"%><body><div class="lefter">    <div class="logo">        <a target="_blank"><img src="images/office.ico" alt="后台管理系统" /></a>    </div></div><div class="righter nav-navicon" id="admin-nav">    <div class="mainer">        <div class="admin-navbar">					<span class="float-right">                    <a class="button button-little bg-main" href="#">前台首页</a>                    <a class="button button-little bg-yellow" href="login.jsp">注销登录</a>                </span>            <ul class="nav nav-inline admin-nav">                <li>                    <a href="index.jsp" class="icon-home"> 开始</a>                    <%@include file = "WEB-INF/jspf/header.jsp" %>                </li>                <li >                    <a href="/DisplayEmployeeServlet" class="icon-male"> 员工</a>                    <%@include file="WEB-INF/jspf/employee.jsp"%>                </li>                <li>                    <a href="seat.jsp" class="icon-wheelchair"> 坐席</a>                    <%@include file="WEB-INF/jspf/seat.jsp"%>                </li>                <li class="active">                    <a href="device.jsp" class="icon-briefcase"> 设备</a>                    <%@include file="WEB-INF/jspf/device.jsp"%>                </li>            </ul>        </div>        <div class="admin-bread">            <span>您好，${user.getEname()}，欢迎您的光临。</span>            <ul class="bread">                <li><a href="index.jsp" class="icon-home"> 开始</a></li>                <li><a href="employee.jsp">员工</a></li>                <li>员工管理</li>            </ul>        </div>    </div></div><div class="admin">    <div class="tab">        <div class="tab-head">            <strong>设备管理</strong>            <ul class="tab-nav">                <li class="active"><a href="#tab-add">设备信息添加</a></li>            </ul>        </div>        <div class="tab-body">            <br />            <div class="tab-panel active" id="tab-add">                <div class="panel-head"><strong>设备信息列表</strong></div>                <form method="post" action="/InsertDeviceServlet">                    <div class="form-group">                        <div class="label x1-move">                            <label for="deviceid">                                设备ID</label>                        </div>                        <div class="field x1-move">                            <input type="text" class="input input-big" size="20" id="deviceid"                                   name="deviceid" size="20"  style="width:500px;"                                   data-validate="required:必填,number:必须是数字,length:长度必须为5,ajax#CheckDeviceIdServlet?deviceid=:ID已被使用"                                   placeholder="输入5位设备ID" />                        </div>                    </div>                    <div class="form-group">                        <div class="label x1-move">                            <label for="devicekind">                                设备类型</label>                        </div>                        <div class="field x1-move">                            <input type="text" class="input input-big" size="20" id="devicekind"                                   name="devicekind" size="30" style="width:500px;"                                   data-validate="required:必填"                                   placeholder="请输入设备类型" />                        </div>                    </div>                    <div class="form-group">                        <div class="label x1-move">                            <label >                                设备放置区域</label>                        </div>                        <div class="field x1-move">                            <div class="button-group radio">                                <label class="button active">                                    <input name="devicearea" value="工作区" checked="checked" type="radio"><span class="icon icon-user"></span> 工作区                                </label>                                <label class="button">                                    <input name="devicearea" value="休息区" type="radio"><span class="icon icon-coffee"></span> 休息区                                </label>                                <label class="button">                                    <input name="devicearea" value="机房区" type="radio"><span class="icon icon-desktop"></span> 机房去                                </label>                                <label class="button">                                    <input name="devicearea" value="仓库区" type="radio"><span class="icon icon-cubes"></span> 仓库区                                </label>                            </div>                        </div>                    </div>                    <div class="form-group">                        <div class="label x1-move">                            <label>                                设置使用状态</label>                        </div>                        <div class="field">                            <div class="button-group radio x1-move">                                <label id="yes" class="button x1-move">                                    <input name="using" value="使用"  type="radio"><span class="icon icon-check"></span>使用                                </label>                                <label id="no" class="button active x1-move">                                    <input name="using" value="未使用" checked="checked" type="radio"><span class="icon icon-times"></span> 未使用                                </label>                            </div>                        </div>                    </div>                    <div class="form-group-optional" style="display: none;">                        <div class="field x1-move">                            <input type="password" class="input input-big" size="20" id="eid"                                   name="eid" size="30" style="width:500px;"                                   data-validate="required:必填"                                   placeholder="请输入使用员工ID" />                        </div>                    </div>                    <div class="form-button x1-move" style="margin-top:10px">                        <button class="button border-green" type="submit">                            提交</button>                        <button class="button form-reset bg-yellow" type="reset">                            重设</button>                    </div>                </form>            </div>        </div>    </div>    <p class="text-right text-gray">基于<a class="text-gray" target="_blank" href="http://www.pintuer.com">拼图前端框架</a>构建</p></div><script>    $("#yes").click(function () {        $(".form-group-optional").show();    });    $("#no").click(function (){        $(".form-group-optional").hide();    });</script></body></html>