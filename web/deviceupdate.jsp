<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@taglib prefix="mytag" uri="/WEB-INF/MyTag"%><!DOCTYPE html><html lang="zh-cn"><%@include file="WEB-INF/jspf/head.jsp"%><body><div class="lefter">    <div class="logo">        <a target="_blank"><img src="images/office.ico" alt="后台管理系统" /></a>    </div></div><div class="righter nav-navicon" id="admin-nav">    <div class="mainer">        <div class="admin-navbar">					<span class="float-right">                    <a class="button button-little bg-main" href="#">前台首页</a>                    <a class="button button-little bg-yellow" href="login.jsp">注销登录</a>                </span>            <ul class="nav nav-inline admin-nav">                <li>                    <a href="index.jsp" class="icon-home"> 开始</a>                    <%@include file = "WEB-INF/jspf/header.jsp" %>                </li>                <li >                    <a href="/DisplayEmployeeServlet" class="icon-male"> 员工</a>                    <%@include file="WEB-INF/jspf/employee.jsp"%>                </li>                <li>                    <a href="seat.jsp" class="icon-wheelchair"> 坐席</a>                    <%@include file="WEB-INF/jspf/seat.jsp"%>                </li>                <li class="active">                    <a href="device.jsp" class="icon-briefcase"> 设备</a>                    <%@include file="WEB-INF/jspf/device.jsp"%>                </li>            </ul>        </div>        <div class="admin-bread">            <span>您好，${user.getEname()}，欢迎您的光临。</span>            <ul class="bread">                <li><a href="index.jsp" class="icon-home"> 开始</a></li>                <li><a href="employee.jsp">员工</a></li>                <li>员工管理</li>            </ul>        </div>    </div></div><div class="admin">    <div class="tab">        <div class="tab-head">            <strong>设备管理</strong>            <ul class="tab-nav">                <li class="active"><a href="#tab-add">设备信息更新</a></li>            </ul>        </div>        <div class="tab-body">            <br />            <div class="tab-panel active" id="tab-add">                <div class="panel-head"><strong>设备信息列表</strong></div>                    <div class="form-group" style="margin-top:10px">                        <div class="field">                            <div class="navbar-form navbar-left x1-move" style="top:15px">                                <form method="post" action="/UpdateDeviceServlet">                                    <input type="text" class="input input-auto " name="id" size="55"                                           data-validate="required:必填,number:必须是数字,length:长度必须为5"                                           placeholder="功能名称">                                    <input type="submit" name="search" value="搜索" class="button bg">                                </form>                            </div>                        </div>                    </div>                <form method="post" action="/UpdateDeviceServlet">                    <div class="form-group"style="clear: both">                        <div class="label x1-move">                            <label for="deviceid">                                设备ID</label>                        </div>                        <div class="field x1-move">                            <input type="text" class="input input-big" readonly="true" size="20" id="deviceid"                                   name="deviceid" value="${device.getDid()}" size="20"  style="width:500px;"                                   data-validate="required:必填,number:必须是数字,length:长度必须为5"                                   placeholder="输入5位设备ID" />                        </div>                    </div>                    <div class="form-group">                        <div class="label x1-move">                            <label for="devicekind">                                设备类型</label>                        </div>                        <div class="field x1-move">                            <input type="text" class="input input-big" size="20" id="devicekind"                                   name="devicekind" size="30" style="width:500px;" readonly="true"                                   value="${device.getDname()}"                                   data-validate="required:必填"                                   placeholder="请输入设备类型" />                        </div>                    </div>                    <div class="form-group">                        <div class="label x1-move">                            <label >                                设备放置区域</label>                        </div>                        <div class="field x1-move">                            <div class="button-group radio">                                <c:if test="${device.getDplace().startsWith('工')}">                                <label class="button active">                                    <input name="devicearea" value="工作区" checked="checked" type="radio"><span class="icon icon-user"></span> 工作区                                </label>                                <label class="button">                                    <input name="devicearea" value="休息区" type="radio"><span class="icon icon-coffee"></span> 休息区                                </label>                                <label class="button">                                    <input name="devicearea" value="机房区" type="radio"><span class="icon icon-desktop"></span> 机房区                                </label>                                <label class="button">                                    <input name="devicearea" value="仓库区" type="radio"><span class="icon icon-cubes"></span> 仓库区                                </label>                                </c:if>                                <c:if test="${device.getDplace().startsWith('休')}">                                    <label class="button">                                        <input name="devicearea" value="工作区"  type="radio"><span class="icon icon-user"></span> 工作区                                    </label>                                    <label class="button active">                                        <input name="devicearea" value="休息区" checked="checked" type="radio"><span class="icon icon-coffee"></span> 休息区                                    </label>                                    <label class="button">                                        <input name="devicearea" value="机房区" type="radio"><span class="icon icon-desktop"></span> 机房区                                    </label>                                    <label class="button">                                        <input name="devicearea" value="仓库区" type="radio"><span class="icon icon-cubes"></span> 仓库区                                    </label>                                </c:if>                                <c:if test="${device.getDplace().startsWith('机')}">                                    <label class="button">                                        <input name="devicearea" value="工作区"  type="radio"><span class="icon icon-user"></span> 工作区                                    </label>                                    <label class="button">                                        <input name="devicearea" value="休息区"  type="radio"><span class="icon icon-coffee"></span> 休息区                                    </label>                                    <label class="button active">                                        <input name="devicearea" value="机房区" checked="checked" type="radio"><span class="icon icon-desktop"></span> 机房区                                    </label>                                    <label class="button">                                        <input name="devicearea" value="仓库区" type="radio"><span class="icon icon-cubes"></span> 仓库区                                    </label>                                </c:if>                                <c:if test="${device.getDplace().startsWith('仓')}">                                    <label class="button">                                        <input name="devicearea" value="工作区"  type="radio"><span class="icon icon-user"></span> 工作区                                    </label>                                    <label class="button">                                        <input name="devicearea" value="休息区"  type="radio"><span class="icon icon-coffee"></span> 休息区                                    </label>                                    <label class="button">                                        <input name="devicearea" value="机房区" type="radio"><span class="icon icon-desktop"></span> 机房区                                    </label>                                    <label class="button active">                                        <input name="devicearea" value="仓库区" checked="checked" type="radio"><span class="icon icon-cubes"></span> 仓库区                                    </label>                                </c:if>                            </div>                        </div>                    </div>                    <div class="form-group">                        <div class="label x1-move">                            <label>                                使用状态(说明:使用状态不可更改!)</label>                        </div>                        <div class="field">                            <div class="button-group radio x1-move" >                                <c:choose>                                    <c:when test="${device.isDstatus()}">                                        <div class="field">                                            <input type="text" class="input input-big" readonly="true" size="20" id="using"                                                   name="using" value="使用中" size="20"  style="width:500px;"/>                                        </div>                                    </c:when>                                    <c:otherwise>                                        <div class="field">                                            <input type="text" class="input input-big" readonly="true" size="20" id="using"                                                   name="using" value="未使用" size="20"  style="width:500px;"/>                                        </div>                                    </c:otherwise>                                </c:choose>                            </div>                        </div>                    </div>                    <div class="form-button x1-move">                        <button class="button border-green" style=" float:left;margin:0px 10px;" type="submit">                            提交</button>                    </div>                </form>                <div>                    <c:choose>                        <c:when test="${device.isDstatus()||device.getDid()==null}">                            <button disabled="disabled" class="button button-small border-red"><span class="icon-trash-o"></span>删除</button>                        </c:when>                        <c:otherwise>                            <a href="/DeleteDeviceServlet?ddid=${device.getDid()}" class="button button-small border-red"><span class="icon-trash-o"></span>删除</a>                        </c:otherwise>                    </c:choose>                </div>            </div>        </div>    </div>    <p class="text-right text-gray">基于<a class="text-gray" target="_blank" href="http://www.pintuer.com">拼图前端框架</a>构建</p></div></body></html>