<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><!DOCTYPE html><html lang="zh-cn"><%@include file="WEB-INF/jspf/head.jsp"%><body><div class="lefter">    <div class="logo">        <a target="_blank"><img src="images/office.ico" alt="后台管理系统" /></a>    </div></div><div class="righter nav-navicon" id="admin-nav">    <div class="mainer">        <div class="admin-navbar">					<span class="float-right">                    <a class="button button-little bg-main" href="#">前台首页</a>                    <a class="button button-little bg-yellow" href="login.jsp">注销登录</a>                </span>            <ul class="nav nav-inline admin-nav">                <li>                    <a href="index.jsp" class="icon-home"> 开始</a>                    <%@include file = "WEB-INF/jspf/header.jsp" %>                </li>                <li class="active">                    <a href="/DisplayEmployeeServlet" class="icon-male"> 员工</a>                    <%@include file="WEB-INF/jspf/employee.jsp"%>                </li>                <li>                    <a href="seat.jsp" class="icon-wheelchair"> 坐席</a>                    <%@include file="WEB-INF/jspf/seat.jsp"%>                </li>                <li><a href="device.jsp" class="icon-briefcase"> 设备</a></li>            </ul>        </div>        <div class="admin-bread">            <span>您好，${user.getEname()}，欢迎您的光临。</span>            <ul class="bread">                <li><a href="index.jsp" class="icon-home"> 开始</a></li>                <li><a href="employee.jsp">员工</a></li>                <li>员工管理</li>            </ul>        </div>    </div></div><div class="admin">    <div class="tab">        <div class="tab-head">            <strong>员工管理</strong>            <ul class="tab-nav" id="emp">                <li class="active" ><a href="#tab-add">员工信息修改</a></li>            </ul>        </div>        <div class="tab-body">            <br />            <div class="tab-panel active " id="tab-visit">                <div class="panel-head"><strong>员工信息列表</strong></div>                <div>                    <div class="form-group" style="margin-top:10px">                        <div class="field">                            <div class="navbar-form navbar-left" style="top:15px;">                                <form method="post" action="/UpdateEmployeeServlet">                                    <input type="text" class="input input-auto " name="seid" size="55"                                           data-validate="required:必填,number:必须是数字,length#==9:长度必须为9,ajax#CheckIdServlet?id=:员工不存在"                                           placeholder="功能名称">                                    <input type="submit" name="search" value="搜索" class="button bg">                                </form>                            </div>                        </div>                    </div>                    <form method="post" action="/UpdateEmployeeServlet">                        <div class="form-group"style="clear:both">                            <div class="label">                                <label for="aid">员工编号</label>                            </div>                            <div class="field">                                <input type="text" class="input" value="${employee.getEid()}" id="aid" name="aid" size="50" readonly="true"/>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label for="ename">姓名</label>                            </div>                            <div class="field">                                <input type="text" class="input" value="${employee.getEname()}" id="ename" name="ename" size="50"/>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label>性别</label>                            </div>                            <div class="field">                                <div class="button-group radio">                                    <c:if test="${employee.getEsex()=='男'}">                                        <label class="button active">                                            <input name="esex" value="男" checked="checked" type="radio"><span class="icon icon-male"></span> 男                                        </label>                                        <label class="button">                                            <input name="esex" value="女" type="radio"><span class="icon icon-female"></span> 女                                        </label>                                    </c:if>                                    <c:if test="${employee.getEsex()=='女'}">                                        <label class="button">                                            <input name="esex" value="男"  type="radio"><span class="icon icon-male"></span> 男                                        </label>                                        <label class="button active">                                            <input name="esex" value="女" checked="checked"type="radio"><span class="icon icon-female"></span> 女                                        </label>                                    </c:if>                                </div>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label for="edept">部门</label>                            </div>                            <div class="field">                                <input type="text" class="input" value="${employee.getEdept()}" id="edept" name="edept"  size="50"/>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label for="eduty">职务</label>                            </div>                            <div class="field">                                <input type="text" class="input" id="eduty" value="${employee.getEduty()}" name="eduty"  size="50"/>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label for="enum">身份证号</label>                            </div>                            <div class="field">                                <input type="text" class="input" value="${employee.getEnum()}" id="enum" name="enum"  size="50"/>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label for="eemail">公司邮箱</label>                            </div>                            <div class="field">                                <input type="text" class="input" value="${employee.getEemail()}" id="eemail" name="eemail"  size="50"/>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label for="etel">个人电话</label>                            </div>                            <div class="field">                                <input type="text" class="input" id="etel" value="${employee.getEtel()}" name="etel"  size="50"/>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label for="eip">个人Ip</label>                            </div>                            <div class="field">                                <input type="text" class="input" value="${employee.getEip()}" id="eip" name="eip"  size="50"/>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label>权限</label>                            </div>                            <div class="field">                                <div class="button-group radio">                                    <c:if test="${employee.getEauthority().startsWith('普')}">                                        <label class="button active">                                            <input name="eauthority" value="普通员工" checked="checked" type="radio"><span class="icon icon-user"></span> 普通员工                                        </label>                                        <label class="button">                                            <input name="eauthority" value="管理员" type="radio"><span class="icon icon-coffee"></span> 管理员                                        </label>                                    </c:if>                                    <c:if test="${employee.getEauthority().startsWith('管')}">                                        <label class="button">                                            <input name="eauthority" value="普通员工" type="radio"><span class="icon icon-user"></span> 普通员工                                        </label>                                        <label class="button active">                                            <input name="eauthority" value="管理员" checked="checked" type="radio"><span class="icon icon-coffee"></span> 管理员                                        </label>                                    </c:if>                                </div>                            </div>                        </div>                        <div class="form-button border-main">                            <button class="button" type="submit" style=" float:left;margin:0px 10px;">确认</button>                        </div>                    </form>                    <div>                        <c:choose>                            <c:when test="${employee.getEid()==null}">                                <button disabled="disabled" class="button button-small border-red"><span class="icon-trash-o"></span>删除</button>                                <button disabled="disabled" class="button button-small border-red"><span class="icon-trash-o"></span>重置密码</button>                            </c:when>                            <c:otherwise>                                <a href="/DeleteEmployeeServlet?deid=${employee.getEid()}" class="button button-small border-red"><span class="icon-trash-o"></span>删除</a>                                <a href="/DeleteEmployeeServlet?ceid=${employee.getEid()}" class="button button-small border-red"><span class="icon-trash-o"></span>重置密码</a>                            </c:otherwise>                        </c:choose>                    </div>                </div>            </div>        </div>    </div>    <p class="text-right text-gray">基于<a class="text-gray" target="_blank" href="http://www.pintuer.com">拼图前端框架</a>构建</p></div></body></html>