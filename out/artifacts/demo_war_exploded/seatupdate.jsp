<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="mytag2" uri="/WEB-INF/MyTag2" %><%@ page contentType="text/html;charset=UTF-8" language="java" %><!DOCTYPE html><html lang="zh-cn"><%@include file="WEB-INF/jspf/head.jsp" %><body><div class="lefter">    <div class="logo">        <a target="_blank"><img src="images/office.ico" alt="后台管理系统"/></a>    </div></div><div class="righter nav-navicon" id="admin-nav">    <div class="mainer">        <div class="admin-navbar">					<span class="float-right">                    <a class="button button-little bg-main" href="#">前台首页</a>                    <a class="button button-little bg-yellow" href="login.jsp">注销登录</a>                </span>            <ul class="nav nav-inline admin-nav">                <li>                    <a href="index.jsp" class="icon-home"> 开始</a>                    <%@include file="WEB-INF/jspf/header.jsp" %>                </li>                <li>                    <a href="/DisplayEmployeeServlet" class="icon-male"> 员工</a>                    <%@include file="WEB-INF/jspf/employee.jsp" %>                </li>                <li class="active">                    <a href="seat.jsp" class="icon-wheelchair"> 坐席</a>                    <%@include file="WEB-INF/jspf/seat.jsp" %>                </li>                <li><a href="device.jsp" class="icon-briefcase"> 设备</a></li>            </ul>        </div>        <div class="admin-bread">            <span>您好，${user.getEname()}，欢迎您的光临。</span>            <ul class="bread">                <li><a href="index.jsp" class="icon-home"> 开始</a></li>                <li><a href="seat.jsp">内容</a></li>                <li>内容管理</li>            </ul>        </div>    </div></div><div class="admin">    <div class="tab">        <div class="tab-head">            <strong>坐席管理</strong>            <ul class="tab-nav">                <li class="active"><a href="#tab-add">坐席信息修改</a></li>            </ul>        </div>        <div class="tab-body">            <br />            <div class="tab-panel active" id="tab-add">                <div class="panel-head"><strong>坐席信息列表</strong></div>                <div>                    <div class="form-group" style="margin-top:10px">                        <div class="field">                            <div class="navbar-form navbar-left" style="top:15px;">                                <form method="post" action="/UpdateSeatServlet">                                    <input type="text" class="input input-auto " name="ssid" size="55"                                           data-validate="required:必填,number:必须是数字,length#==5:长度必须为5"                                           placeholder="功能名称">                                    <input type="submit" name="search" value="搜索" class="button bg">                                </form>                            </div>                        </div>                    </div>                    <form method="post" action="/UpdateSeatServlet">                        <div class="form-group">                            <div class="label" style="clear:both">                                <label for="sid">坐席ID</label>                            </div>                            <div class="field">                                <input type="text" class="input" id="sid" name="sid" readonly="readonly" value="${seat.getSid()}" size="50" placeholder="设备编号" data-validate="required:必填" />                            </div>                        </div>                        <div class="form-group"style="clear:both">                            <div class="label">                                <label for="eid">员工编号</label>                            </div>                            <div class="field">                                <input type="text" class="input" value="${seat.getEid()}" id="eid" name="eid" size="50"/>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label for="area">坐席位置</label>                            </div>                            <div class="field">                                <mytag2:my area="${seat.getSarea()}"/>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label for="sstatus">坐席状态</label>                            </div>                            <div class="field">                                <input type="text" class="input" value="${seat.getSstatus()?"是":"否"}" id="sstatus" name="sstatus"  size="50"/>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label for="isusing">员工在席</label>                            </div>                            <div class="field">                                <input type="text" class="input" id="isusing" value="${seat.getIsusing()?"是":"否"}" name="isusing"  size="50"/>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label for="isleava">员工请假</label>                            </div>                            <div class="field">                                <input type="text" class="input" id="isleave" value="${seat.getIsleave()}" name="isleave"  size="50"/>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label for="sstart">开始时间</label>                            </div>                            <div class="field">                                <input type="text" class="input" id="sstart" value="${seat.getSstart()}" name="sstart"  size="50"/>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label for="send">结束时间</label>                            </div>                            <div class="field">                                <input type="text" class="input" id="send" value="${seat.getSend()}" name="send"  size="50"/>                            </div>                        </div>                        <div class="form-group">                            <div class="label">                                <label for="sremark">备注</label>                            </div>                            <div class="field">                                <input type="text" class="input" id="sremark" value="${seat.getSremark()}" name="sremark"  size="50"/>                            </div>                        </div>                        <div class="form-button border-main">                            <c:choose>                                 <c:when test="${seat.getSid()==null}">                                    <button class="button" type="submit" disabled="disabled" style=" float:left;margin:0px 10px;">确认</button>                                 </c:when>                                 <c:otherwise>                                    <button class="button" type="submit" style=" float:left;margin:0px 10px;">确认</button>                                 </c:otherwise>                            </c:choose>                        </div>                    </form>                </div>            </div>        </div>    </div>    <br/>    <p class="text-right text-gray">基于<a class="text-gray" target="_blank" href="http://www.pintuer.com">拼图前端框架</a>构建    </p></div></body></html>